pipeline {
    agent any

    stages {
        stage(‘cloneProject’) {
            steps {
                 git branch:’master’, url:’https://github.com/SHNavyashree/Amazon.git'
            }
        }
      
        stage(‘clean’) {
            steps {
                  sh ‘mvn clean’
            }
        }

        stage(‘compile’) {
            steps {
                  sh ‘mvn compile’
            }
        }

        stage(‘test’) {
            steps {
                  sh ‘mvn test’
            }
        }

        stage(‘build’) {
            steps {
                  sh ‘mvn clean install’
            }
        }
       
    }
}

